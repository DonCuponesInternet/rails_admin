.complete-width {
  width: 100% !important
}

.coupon-publication-status-expired {
  background-color: #C30D0D !important;
  color: white !important;
}

.coupon-publication-status-online {
  background-color: #5cb85c !important;
  color: white !important;
}

.coupon-publication-status-pending {
  background-color: rgb(245, 238, 28) !important;
}

.sprite-flags {
  background-image: url('data:image/png;base64,');
  display: inline-block;
}

@media (min--moz-device-pixel-ratio: 1.5), (-o-min-device-pixel-ratio: 3/2), (-webkit-min-device-pixel-ratio: 1.5), (min-device-pixel-ratio: 1.5), (min-resolution: 1.5dppx) {
  .sprite-flags {
   background-image: url('data:image/png;base64,');
   background-size: 120px 20px;
 }
}

.flag-main-cl {
  width: 30px;
  height: 20px;
  background-position: 0 0;
}

.flag-main-co {
  width: 30px;
  height: 20px;
  background-position: -30px 0;
}

.flag-es {
  width: 30px;
  height: 20px;
  background-position: -60px 0;
}

.flag-main-mx {
  width: 30px;
  height: 20px;
  background-position: -90px 0;
}

.el-universal-menu {
  background-color: #DFEFFF
}

.de10-menu {
  background-color: white
}

.milenio-menu {
  $top: rgb(196, 25, 0);
  $bottom: rgb(150, 19, 0);
  
  /* IE10+ */ 
  background-image: -ms-linear-gradient(top, $top 0%, $bottom 100%);

  /* Mozilla Firefox */ 
  background-image: -moz-linear-gradient(top, $top 0%, $bottom 100%);

  /* Opera */ 
  background-image: -o-linear-gradient(top, $top 0%, $bottom 100%);

  /* Webkit (Safari/Chrome 10) */ 
  background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, $top), color-stop(100, $bottom));

  /* Webkit (Chrome 11+) */ 
  background-image: -webkit-linear-gradient(top, $top 0%, $bottom 100%);

  /* W3C Markup */ 
  background-image: linear-gradient(to bottom, $top 0%, $bottom 100%);
}

.locale-tab-with-value{
  background-color: #f1fff1
}
